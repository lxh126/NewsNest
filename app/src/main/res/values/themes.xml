<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Androidnew" parent="Theme.MaterialComponents.Light.NoActionBar">//"Theme.MaterialComponents.Light.NoActionBar.Bridge""Theme.AppCompat.Light"
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_200</item>
        <item name="colorPrimaryVariant">@color/purple_500</item>
        <item name="colorOnPrimary">@color/purple_700</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/white</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Androidnew.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Androidnew.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Androidnew.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="CustomButtonStyle" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/baseline_rectangle_notselected</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="CustomButtonStyle2" parent="Widget.AppCompat.Button">
        <item name="android:background">@drawable/baseline_rectangle_selected</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="AppTheme.ButtonTheme" parent="Widget.AppCompat.Button">
        <!-- 覆盖影响按钮颜色的属性 -->
        <item name="colorControlNormal">#00000000</item>
        <item name="colorControlActivated">#00000000</item>
        <item name="colorControlHighlight">#00000000</item>
        <item name="colorPrimary">#00000000</item>
        <item name="colorPrimaryVariant">#00000000</item>
        <item name="colorOnPrimary">#00000000</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#00000000</item>
        <item name="colorSecondaryVariant">#00000000</item>
        <item name="colorOnSecondary">#00000000</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">#00000000</item>
        <!-- 其他可能影响的属性 -->
    </style>
</resources>